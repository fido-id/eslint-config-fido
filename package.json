{
  "name": "@fido.id/eslint-config-fido",
  "version": "2.0.0",
  "description": "Fido.id shared ESLint configs, aiming to improve code quality of react applications",
  "main": "src/index.js",
  "repository": {
    "url": "https://github.com/fido-id/eslint-config-fido"
  },
  "engines": {
    "node": ">=18.18"
  },
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslintconfig",
    "eslint-config",
    "fido.id",
    "trustfull.com"
  ],
  "author": {
    "email": "marco@fido.id",
    "name": "Marco Cotrufo"
  },
  "contributors": [
    {
      "name": "Enrico Martinelli",
      "email": "e.martinelli@trustfull.com"
    },
    {
      "name": "Alessandro Ceruti",
      "email": "a.ceruti@trustfull.com"
    }
  ],
  "scripts": {
    "lint": "eslint src/**/*.js tests/**/*.js",
    "lint:fix": "eslint --fix src/**/*.js tests/**/*.js",
    "commit": "cz",
    "semantic-release": "semantic-release",
    "test": "jest"
  },
  "peerDependencies": {
    "eslint": ">=9.34.0 <10.0.0",
    "prettier": ">=3.6",
    "typescript": ">=4.2.0"
  },
  "dependencies": {
    "@types/node": "20",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "neostandard": "^0.12.2"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "^11.0.0",
    "@semantic-release/npm": "^12.0.0",
    "@typescript-eslint/parser": "^8.42.0",
    "commitizen": "^4.3.0",
    "conventional-changelog-conventionalcommits": "^9.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.34.0",
    "jest": "^30.0.5",
    "prettier": "^3.6.0",
    "semantic-release": "^24.0.0",
    "typescript": "^5.9.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits"
        }
      ],
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md"
          ]
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
