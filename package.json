{
  "name": "@fido.id/eslint-config-fido",
  "version": "1.2.1",
  "description": "Fido.id shared ESLint configs, aiming to improve code quality of react applications",
  "main": "src/index.js",
  "repository": {
    "url": "https://github.com/fido-id/eslint-config-fido"
  },
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslintconfig",
    "eslint-config",
    "fido.id"
  ],
  "author": {
    "email": "marco@fido.id",
    "name": "Marco Cotrufo"
  },
  "contributors": [
    {
      "name": "Enrico Martinelli",
      "email": "e.martinelli@trustfull.com"
    },
    {
      "name": "Alessandro Ceruti",
      "email": "a.ceruti@trustfull.com"
    }
  ],
  "scripts": {
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint --fix src/**/*.js",
    "commit": "cz",
    "semantic-release": "semantic-release"
  },
  "peerDependencies": {
    "eslint": ">=8.57.0",
    "prettier": ">=3.2",
    "typescript": ">=4.2.0"
  },
  "dependencies": {
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.34.1",
    "typescript-eslint": "^7.8.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "^10.0.0",
    "@semantic-release/npm": "^12.0.0",
    "commitizen": "^4.3.0",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^17.6.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "prettier": "^3.2.0",
    "semantic-release": "^23.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits"
        }
      ],
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md"
          ]
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}